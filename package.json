{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-icons": "^3.7.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0",
    "with-router": "^1.0.1"
  },
  "scripts": {
    "copy": "sudo rm -r ../server/build && mv build ../server",
    "start": "react-scripts start",
    "build-dev": "dotenv -e .env.development react-scripts build && npm run copy", 
    "build-prod": "dotenv -e .env.production react-scripts build && npm run copy", 
    "build": "react-scripts build && sudo rm -r ../server/build && mv build ../server",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
},
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
